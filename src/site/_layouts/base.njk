<!doctype html>
<html class="no-js" lang="en">
  <head>
    {% block head %}
      {% include "head.njk" %}
    {% endblock %}
  </head>

  <body>
    {% block header %}{% endblock %}

    {% block content %}
      {{ content | safe }}
    {% endblock %}

    {% block footer %}{% endblock %}

    {% block end %}
      {% include "end.njk" %}
    {% endblock %}
  </body>
</html>
